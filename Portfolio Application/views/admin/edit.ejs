<%- include('../partials/header') -%>

<h1>Added Course</h1>
<form method="POST" action="/admin/edit/<%= course._id %>" class=" mb-5">
  <div class="form-group">
    <label for="title">Title</label>
    <input type="text" class="form-control" value="<%= course.title %>" name="title" placeholder="Enter Title"/>
  </div>
  <div class="form-group">
    <label for="description">Description</label>
    <input type="text" class="form-control" value="<%= course.description %>" name="description" placeholder="Enter Description"
    />
  </div>
  <div class="form-group">
    <label for="service">Service</label>
    <input type="service" class="form-control" value="<%= course.service %>" name="service" placeholder="Enter Service"
    />
  </div>
  <div class="form-group">
    <label for="client">Client</label>
    <input type="text" class="form-control" value="<%= course.client %>" name="client" placeholder="Enter Client"
    />
  </div>
  <div class="form-group">
    <label for="url">Project url</label>
    <input type="text" class="form-control" value="<%= course.projectUrl %>" name="projectUrl" placeholder="Enter ProjectUrl"/>
  </div>
  <div class="form-group">
    <input type="file" class="form-control" value="<%= course.imageUrl %>" name="imageUrl" placeholder="Enter File" />
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>

<%- include('../partials/footer') -%>
